import sqlite3
conn = sqlite3.connect('napthe.db')
cursor = conn.cursor()
cursor.execute('''CREATE TABLE IF NOT EXISTS napthe_requests
                  (request_id TEXT PRIMARY KEY, 
                   discord_user_id TEXT, 
                   channel_id TEXT, 
                   telco TEXT, 
                   seri TEXT, 
                   mathe TEXT, 
                   amount INTEGER, 
                   received_amount INTEGER DEFAULT 0, 
                   status TEXT DEFAULT 'processing', 
                   message TEXT)''')
conn.commit()
conn.close()