{
  "version": 2,
  "builds": [
    {
      "src": "api/callback.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "api/callback.py"
    },
    {
      "src": "/callback",
      "dest": "api/callback.py"
    },
    {
      "src": "/callback-check",
      "dest": "api/callback.py"
    }
  ]
}
